<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppHuac.Views.Dashboard"
             xmlns:pages="clr-namespace:AppHuac.Views" 
            NavigationPage.HasNavigationBar="False"
            >
    <FlyoutPage.Flyout>
      <pages:DashboardFlyout x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage BarBackgroundColor="#00B8DF">
         <x:Arguments>
            <pages:DashboardDetail />
         </x:Arguments>
      </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>